{
  "expo": {
    "name": "astrosnax",
    "slug": "astrosnax",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "astrosnax",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "enableFullScreenImage_legacy": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alleyhector1981.astrosnax"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alleyhector1981.astrosnax"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
      // [
      //   "expo-splash-screen",
      //   {
      //     "image": "./assets/images/splash.png",
      //     "resizeMode": "contain",
      //     "backgroundColor": "#ffffff",
      //     "enableFullScreenImage": true
      //   }
      // ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7cbbce6b-d492-4b9a-80f0-cf8ab3db5cc6"
      }
    },
    "owner": "alleyhector1981"
  }
}
